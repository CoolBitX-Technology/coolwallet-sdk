    "@babel/preset-typescript": "^7.7.7",
    "@coolwallet/core": "^1.1.23"
  },
  "dependencies": {
    "@types/bn.js": "^5.1.1",
    "bip32": "^2.0.4",
    "bn.js": "^5.2.1"
