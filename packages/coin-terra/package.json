    "@babel/types": "^7.17.0",
    "@coolwallet/core": "1.1.8",
    "@terra-money/terra.js": "^3.0.8",
    "@types/node": "^14.0.13",
    "esbuild": "^0.14.38",
    "simple-git": "^3.7.1",
    "typescript": "^4.6.2"
  },
